@model List<EmployeeViewModel>

<table class="table table-striped table-inverse table-responsive hovered">
    <thead class="thead-inverse">
        <tr>

            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
                    <tr>

            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Title</td>
            <td>@item.EnterDate</td>


        </tr>
        }

    </tbody>
</table>