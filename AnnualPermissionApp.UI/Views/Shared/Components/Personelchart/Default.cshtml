@if (ViewBag.Proportion <=25)
{
<div class="progress mx-auto my-auto mt-2">
    <div class="progress-bar bg-success" role="progressbar" style="width:@(ViewBag.Proportion)%;" aria-valuenow="25"
        aria-valuemin="0" aria-valuemax="100">
        @(ViewBag.Proportion)%
    </div>
</div>
}
else if(ViewBag.Proportion>25 && ViewBag.Proportion<=50)
{
<div class="progress mx-auto my-auto mt-2">
    <div class="progress-bar bg-info" role="progressbar" style="width:@(ViewBag.Proportion)%;" aria-valuenow="25"
        aria-valuemin="0" aria-valuemax="100">
        @(ViewBag.Proportion)%
    </div>
</div>
}
else if(ViewBag.Proportion>50 &&  ViewBag.Proportion<=75)
{
<div class="progress mx-auto my-auto mt-2">
    <div class="progress-bar bg-warning" role="progressbar" style="width:@(ViewBag.Proportion)%;" aria-valuenow="25"
        aria-valuemin="0" aria-valuemax="100">
        @(ViewBag.Proportion)%
    </div>
</div>
}
else
{
    <div class="progress mx-auto my-auto mt-2">
    <div class="progress-bar bg-danger" role="progressbar" style="width:@(ViewBag.Proportion)%;" aria-valuenow="25"
        aria-valuemin="0" aria-valuemax="100">
        @(ViewBag.Proportion)%
    </div>
</div>
}



